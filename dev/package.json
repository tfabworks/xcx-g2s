{
  "name": "xcx-g2s",
  "version": "0.11.0",
  "scripts": {
    "register": "npx xcratch-register --id=g2s --gui=../scratch-gui --block=./src/vm/extensions/block --entry=./src/gui/lib/libraries/extensions/entry --core --link --use=extension-support util",
    "build": "npx xcratch-build --module=g2s --gui=../scratch-gui --block=./src/vm/extensions/block --entry=./src/gui/lib/libraries/extensions/entry",
    "build-dev": "npx xcratch-build --module=g2s-dev --gui=../scratch-gui --block=./src/vm/extensions/block --entry=./src/gui/lib/libraries/extensions/entry"
  },
  "devDependencies": {
    "xcratch-build": "^0.5.0",
    "xcratch-register": "^0.4.2"
  },
  "dependencies": {
    "@serialport/stream": "^9.2.4",
    "firmata-io": "https://gitpkg.now.sh/yokobond/firmata.js/packages/firmata-io?xcx-g2s",
    "long": "^5.2.0",
    "web-serial-binding": "github:jaz303/web-serial-binding"
  }
}
